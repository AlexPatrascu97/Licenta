@model IEnumerable<Licenta_Front_End.Models.Member>

@{
	ViewData["Title"] = "Members";
}


<div class="table-responsive box">
	<table class="table thead-light table-striped table-white box2">

		<thead>

			<tr class="table-warning">
				<th>
					Name
				</th>

				<th>
					Job Title
				</th>
				<th>
					Departament Name
				</th>
				<th>
					@Html.DisplayNameFor(model => model.Role)
				</th>
				<th>
					Project Name
				</th>

				<th>
					Email
				</th>





			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{
				<tr>
					<td>
						@Html.DisplayFor(modelItem => item.FirstName)
						@Html.DisplayFor(modelItem => item.LastName)

					</td>
					<td>
						@Html.DisplayFor(modelItem => item.JobTitle)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.DepartamentName)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Role)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.ProjectName)
					</td>
					<th>
						@Html.DisplayFor(modelItem => item.Email)
					</th>


				</tr>

			}

		</tbody>

	</table>


	<div class="butoane">
		<br>
		<a asp-action="IndexEmpProjects" class="btn btn-warning" style = "background-color: #CC621B; border-color: #CC621B; color: black;">Back to List</a>

	</div>



</div>
